{
  "name": "nuxt-server-extension",
  "type": "module",
  "version": "0.1.0",
  "description": "Database utilities, API views, admin UI, and more for Nuxt",
  "license": "MIT",
  "homepage": "https://nxse.vercel.app",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "prepack": "pnpm build",
    "dev": "nuxi dev docs",
    "play": "nuxi dev playground",
    "build:docs": "nuxi generate docs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare docs",
    "release": "release-it",
    "test": "vitest"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^1.0.5",
    "@nuxt/kit": "^3.8.2",
    "drizzle-orm": "^0.29.1",
    "knitwork": "^1.0.0",
    "sirv": "^2.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.4.5",
    "@iconify-json/carbon": "^1.1.26",
    "@nuxt/devtools": "latest",
    "@nuxt/devtools-ui-kit": "^1.0.5",
    "@nuxt/module-builder": "^0.5.4",
    "@nuxt/schema": "^3.8.2",
    "@nuxt/test-utils": "^3.8.1",
    "changelogen": "^0.5.5",
    "eslint": "^8.55.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "nuxt": "^3.8.2"
  }
}
